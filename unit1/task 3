#1
while x % 2 == 1 and x % 10 != 5

#2
for i in range(10)

#3
k, n = map(int, input().split())
s = 0
while k <= n:
    if k % 2 == 1:
        s += k
    k += 1
print(s)
Ответ: 768182441

#4
n = int(input())
f = 1
for i in range(2, n + 1):
    f *= i
print(f)
Ответ: 3628800

#5
n = int(input())
a, b = 1, 1
i = 3
while i <= n:
    a, b = b, a + b
    i += 1
print(b)
Ответ: 55
